{
  "compilerOptions": {
    "target": "es6",
    //   "module": "commonjs",                    
    "lib": [
      "es6"
    ],
    "allowJs": true,
    "outDir": "build",
    //   "rootDir": "src",
    "moduleResolution": "node",
    "strict": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*"
  ],
  "ts-node": {
    "esm": true
  },
  //   "lib": ["esnext"]
}